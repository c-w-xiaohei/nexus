{
  "name": "@nexus-js/chrome",
  "version": "0.1.1",
  "description": "Chrome extension adapter for Nexus framework",
  "private": false,
  "license": "MIT",
  "author": "Carson Yu",
  "repository": {
    "type": "git",
    "url": "https://github.com/c-w-xiaohei/nexus.git",
    "directory": "packages/chrome"
  },
  "keywords": [
    "nexus",
    "chrome-extension",
    "ipc",
    "cross-context"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest run",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@nexus-js/core": "workspace:*"
  },
  "devDependencies": {
    "@types/chrome": "0.1.36",
    "@vitejs/plugin-react-swc": "4.2.3",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vite-plugin-dts": "4.5.4",
    "vitest": "4.0.18"
  },
  "peerDependencies": {
    "@nexus-js/core": "^0.1.1"
  }
}
